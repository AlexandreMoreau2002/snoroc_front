{
  "name": "snoroc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.13.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.5",
    "react-scripts": "^5.0.1",
    "react-snowfall": "^2.4.0",
    "sweetpagination": "^2.0.1",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "sass": "sass --watch src/scss/main.scss src/css/index.css",
    "dev": "concurrently \"npm run sass\" \"npm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--no-deprecation react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/jestWarningFilter.js",
      "!src/**/export.js"
    ],
    "moduleNameMapper": {
      "^punycode$": "punycode/",
      "^react-router-dom$": "<rootDir>/node_modules/react-router-dom/dist/index.js",
      "^react-router$": "<rootDir>/node_modules/react-router/dist/development/index.js",
      "^react-router/dom$": "<rootDir>/node_modules/react-router/dist/development/dom-export.js",
      "^react-snowfall$": "<rootDir>/src/__mocks__/react-snowfall.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/dom": "^10.4.1",
    "ajv": "^6.12.6",
    "ajv-keywords": "^3.5.2",
    "concurrently": "^9.2.1",
    "punycode": "^2.3.1"
  }
}
